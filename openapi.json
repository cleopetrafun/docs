        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/Transactions"
          }
        }
      }
    },
    "/dammv2/claim": {
      "post": {
        "tags": ["DAMMv2"],
        "summary": "Claim DAMMv2 fees",
        "operationId": "postDammv2Claim",
        "requestBody": {
          "$ref": "#/components/requestBodies/Dammv2Claim"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/Transactions"
          }
        }
      }
    },
    "/dammv2/position": {
      "get": {
        "tags": ["DAMMv2"],
        "summary": "Retrieve a DAMMv2 position",
        "operationId": "getDammv2Position",
        "parameters": [
          {
            "name": "position",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/Dammv2Position"
          }
        }
      }
    },
    "/positions/": {
      "get": {
        "tags": ["Positions"],
        "summary": "List positions for a user",
        "operationId": "getPositions",
        "parameters": [
          {
            "name": "user",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "programs",
            "in": "query",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["dammv2", "dlmm"]
              },
              "default": ["dammv2", "dlmm"]
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "default": 10,
              "minimum": 1
            }
          },
          {
            "name": "page",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "default": 1,
              "minimum": 1
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/Positions"
          }
        }
      }
    },
    "/pools/token": {
      "get": {
        "tags": ["Pools"],
        "summary": "List pools for a token",
        "operationId": "getPoolsToken",
        "parameters": [
          {
            "name": "token",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "programs",
            "in": "query",
            "required": true,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["dlmm", "dammv2"]
              },
              "default": ["dlmm", "dammv2"]
            }
          },
          {
            "name": "order_by",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "default": "desc",
              "enum": ["desc", "asc"]
            }
          },
          {
            "name": "sort_by",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "default": "volume",
              "enum": ["tvl", "volume", "feetvlratio"]
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "default": 10,
              "minimum": 1
            }
          },
          {
            "name": "page",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "default": 1,
              "minimum": 1
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/PoolsByToken"
          }
        }
      }
    }
  },
