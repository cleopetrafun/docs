        "requestBody": {
          "$ref": "#/components/requestBodies/DlmmRebalance"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/DlmmRebalance"
          }
        }
      }
    },
    "/dlmm/position": {
      "get": {
        "tags": ["DLMM"],
        "summary": "Retrieve a DLMM position",
        "operationId": "getDlmmPosition",
        "parameters": [
          {
            "name": "position",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/DlmmPosition"
          }
        }
      }
    },
    "/dammv2/initialize": {
      "post": {
        "tags": ["DAMMv2"],
        "summary": "Initialize a DAMMv2 position",
