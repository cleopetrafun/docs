  "paths": {
    "/dlmm/initialize": {
      "post": {
        "tags": ["DLMM"],
        "summary": "Initialize a DLMM position",
        "operationId": "postDlmmInitialize",
        "requestBody": {
          "$ref": "#/components/requestBodies/DlmmInitialize"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/DlmmInitialize"
          }
        }
      }
    },
    "/dlmm/close": {
      "post": {
        "tags": ["DLMM"],
        "summary": "Close a DLMM position",
        "operationId": "postDlmmClose",
        "requestBody": {
          "$ref": "#/components/requestBodies/DlmmClose"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/Transactions"
          }
        }
      }
    },
    "/dlmm/claim": {
      "post": {
        "tags": ["DLMM"],
        "summary": "Claim DLMM fees",
        "operationId": "postDlmmClaim",
        "requestBody": {
          "$ref": "#/components/requestBodies/DlmmClaim"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/Transactions"
          }
        }
      }
    },
    "/dlmm/rebalance": {
      "post": {
        "tags": ["DLMM"],
        "summary": "Rebalance a DLMM position",
